@model IEnumerable<E_learning_portal.Models.MyModels.Material>

@{
    ViewBag.Title = "Название";
}

<h2>Название</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Details", "Content", new { id=item.MaterialId }) |
        </td>
    </tr>
}

</table>
